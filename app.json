{
  "expo": {
    "name": "android-frontend",
    "slug": "android-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RecycleRangerLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/AndroidS.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.androidfontend.recycleranger"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/RecycleRangerLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.recycleranger.androidfontend"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a686b036-6a59-4e38-b009-0c8ae1ba478a"
      }
    }
  }
}
